<idea-plugin url="https://github.com/koxudaxi/pyscript-pycharm-plugin">
    <id>com.koxudaxi.pyscript</id>
    <name>PyScript</name>
    <vendor email="koaxudai@gmail.com">Koudai Aono @koxudaxi</vendor>

    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.python</depends>
    <depends>org.intellij.intelliLang</depends>
    <extensions defaultExtensionNs="com.intellij">
        <xml.tagNameProvider implementation="com.koxudaxi.pyscript.PyScriptTagNameProvider"/>
        <xml.elementDescriptorProvider implementation="com.koxudaxi.pyscript.PyScriptElementDescriptorProvider"/>
   </extensions>
    <extensions defaultExtensionNs="org.intellij.intelliLang">
        <injectionConfig config="META-INF/only-injection.xml" />
    </extensions>
</idea-plugin>
